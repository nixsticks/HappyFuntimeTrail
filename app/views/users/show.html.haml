.darkblue
  .center.title
    %h1= @user.username
    = image_tag @user.avatar.url(:medium)
    - if @user == current_user
      %p= link_to 'Edit Profile', edit_user_path(@user)
  .user.col-md-4.col-md-offset-4
    - if @user == current_user
      = render 'current_user'
    - else
      = render 'show'